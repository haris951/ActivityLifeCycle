<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".addhome.AddHome">

    <TextView
        android:id="@+id/textview1_home"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:paddingStart="20sp"
        android:paddingTop="15sp"
        android:paddingEnd="10sp"
        android:text="@string/home"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.458"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:layout_width="40sp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:paddingStart="5sp"
        android:paddingTop="25sp"
        android:paddingEnd="5sp"
        app:drawableLeftCompat="@drawable/baseline_keyboard_backspace_24"
        app:layout_constraintEnd_toStartOf="@+id/textview1_home"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:onClick="openMain9"
        android:paddingTop="20sp"
        android:text="@string/edit"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.756"
        app:layout_constraintStart_toEndOf="@+id/textview1_home"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/address_home"
        android:layout_width="match_parent"
        android:layout_height="30sp"
        android:layout_marginTop="20dp"
        android:background="@color/grey"
        android:gravity="center_vertical"
        android:paddingStart="15sp"
        android:text="@string/address"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/textview1_home"
        tools:ignore="RtlSymmetry" />

    <TextView
        android:id="@+id/textview2home"
        android:layout_width="match_parent"
        android:layout_height="80sp"
        android:drawablePadding="10sp"
        android:hint="@string/address_will_be_shown_here"
        android:paddingStart="10sp"
        android:paddingEnd="5sp"
        android:gravity="center_vertical"
        app:layout_constraintTop_toBottomOf="@+id/address_home"
        app:drawableLeftCompat="@drawable/baseline_location_on_14" />
    <TextView
        android:id="@+id/notifications_home"
        android:layout_width="match_parent"
        android:layout_height="30sp"
        android:gravity="center_vertical"
        android:paddingStart="15sp"
        app:layout_constraintTop_toBottomOf="@id/textview2home"
        android:text="@string/notifications"
        android:textSize="15sp"
        android:background="@color/grey"
        tools:ignore="RtlSymmetry" />

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/id_map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="0dp"
        android:layout_height="250sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/notifications_home"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textview3home"
        android:text="@string/be_sure_your_family_members_get"
        android:textSize="18sp"
        android:textStyle="bold"
        android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="45sp"
        android:layout_marginTop="20sp"
        app:layout_constraintTop_toBottomOf="@+id/id_map"
        />
    <TextView
        android:id="@+id/textview4home"
        android:text="@string/notification"
        android:textStyle="bold"
        android:textSize="18sp"
        android:gravity="center_horizontal"
        android:layout_width="match_parent"
        android:layout_height="45sp"
        app:layout_constraintTop_toBottomOf="@+id/textview3home"
        />

    <TextView
        android:id="@+id/textview5home"
        android:layout_width="match_parent"
        android:layout_height="45sp"
        android:gravity="center"
        android:text="@string/please_invite_members_to_set"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textview4home" />

    <TextView
        android:id="@+id/textview6home"
        android:layout_width="match_parent"
        android:layout_height="45sp"
        android:text="@string/notification"
        android:textSize="18sp"
        android:gravity="center_horizontal"
        app:layout_constraintTop_toBottomOf="@+id/textview5home"
        />

    <Button
        android:layout_width="300sp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="76sp"
        android:text="@string/add_a_new_member"
        android:textSize="15sp"
        android:backgroundTint="@color/green"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.666"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/textview6home"
        app:layout_constraintVertical_bias="0.168" />


</androidx.constraintlayout.widget.ConstraintLayout>